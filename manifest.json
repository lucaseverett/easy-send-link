{
  "manifest_version": 3,
  "name": "Easy Send Link",
  "version": "1.0.0",
  "description": "Click the button to send a link to the current page using your default mailto handler",
  "permissions": ["activeTab", "offscreen"],
  "host_permissions": ["<all_urls>"],
  "icons": {
    "16": "mail_C7C7C7.png",
    "32": "mail_C7C7C7.png",
    "48": "mail_C7C7C7.png",
    "128": "mail_C7C7C7.png"
  },
  "action": {
    "default_title": "Send link to current page via email"
  },
  "background": {
    "service_worker": "background.js"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "key": ""
}
